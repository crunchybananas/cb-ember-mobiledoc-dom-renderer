{
  "name": "cb-ember-mobiledoc-editor",
  "version": "0.0.7",
  "description": "Ember Mobiledoc Wrapper",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/crunchybananas/cb-ember-mobiledoc-editor",
  "license": "MIT",
  "author": "Crunchy Bananas",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "files": [
    "addon-main.js",
    "dist"
  ],
  "exports": {
    "./*": "./dist/*",
    "./addon-main.js": "./addon-main.js"
  },
  "scripts": {
    "build": "rollup --config",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "rollup --config --watch",
    "test": "echo 'Addon does not have tests, run tests in test-app'",
    "prepare": "yarn build"
  },
  "dependencies": {
    "@ember/render-modifiers": "^2.0.0",
    "@embroider/addon-shim": "^1.0.0",
    "@glimmer/component": "^1.1.2",
    "ember-cli-string-helpers": "^6.1.0",
    "ember-modifier": "^3.2.7",
    "ember-style-modifier": "^0.8.0",
    "lodash": "^4.17.4",
    "mobiledoc-kit": "^0.14.0"
  },
  "devDependencies": {
    "@babel/core": "7.18.5",
    "@babel/plugin-proposal-class-properties": "7.17.12",
    "@babel/plugin-proposal-decorators": "7.18.2",
    "@embroider/addon-dev": "1.8.0",
    "@rollup/plugin-babel": "^5.3.1",
    "babel-eslint": "10.1.0",
    "ember-template-lint": "4.10.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-ember": "10.6.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-qunit": "7.3.0",
    "npm-run-all": "4.1.5",
    "rollup": "2.75.6"
  },
  "peerDependencies": {
    "ember-source": "~4.0.0"
  },
  "engines": {
    "node": ">= 14"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.js",
    "app-js": {
      "./components/mobiledoc/editor.js": "./dist/_app_/components/mobiledoc/editor.js",
      "./modifiers/mobiledoc-editor.js": "./dist/_app_/modifiers/mobiledoc-editor.js"
    }
  }
}
